algoritmo "Atividade 4"

// Data : 18/04/2017

var
  a,b,c:inteiro
inicio
      Escreva("Escreva um numero: ")
      leia(a)
      Escreva("Escreva um numero: ")
      leia(b)
      Escreva("Escreva um numero: ")
      leia(c)
      a<-a+b+c
      Se a>50 entao
         escreva("A soma dos numeros é maior que 50")
      FimSE
fimalgoritmo
