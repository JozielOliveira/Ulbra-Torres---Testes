algoritmo "semnome"
var
  A,B,C: inteiro
  M:real
inicio
      Escreva("Escreva um valor A: ")
      leia(A)
      Escreva("Escreval um valor B: ")
      leia(B)
      Escreva("Escreva um valor C: ")
      leia(C)
      M<-(A+B+C)/3
      Escreva("A media de ",A,", ",B," e ",C," é ",M)
fimalgoritmo
