algoritmo "Atividade %"
// Data : 11/04/2017
var
   A,B: inteiro
inicio
   Escreva("Escreva um numero: ")
   leia(A)
   Escreva("Escreva um numero: ")
   leia(B)
   Se A=B entao
      Escreva("NÚMERO IGUAIS")
   Senao
      Se A>B entao
         Escreva(A)
      Senao
         Escreva(B)
      Fimse
   FimSe
fimalgoritmo
