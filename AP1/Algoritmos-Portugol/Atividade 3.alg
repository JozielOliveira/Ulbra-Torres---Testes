algoritmo "Atividade 3"
var
    A,B,C,D:Inteiro
inicio
      Escreva("Escreva o valor A: ")
      leia(A)
      Escreva("Escreva o valor B: ")
      leia(B)
      Escreva("Escreva o valor C: ")
      leia(C)
      D<-B*2-4*A*C
      Escreva ("O resultado de ",B,"*2-4*",A,"*",C," é ",D)
fimalgoritmo
