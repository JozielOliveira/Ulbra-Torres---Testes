algoritmo "semnome"
// Data : 09/05/2017
var
   a,b,i: inteiro
inicio
      repita
            Escreva("Escreva um numero : ")
            leia(a)
            Escreva("Escreva um numero : ")
            leia(b)
            i<-i+1
      ate ((a>0) e (a<10)) e ((b>0) e (b<10))
      Escreva("Tentativas: ",i)
fimalgoritmo
